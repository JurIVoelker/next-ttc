{
  "name": "next-ttc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -p \"[{name}]\" -n \"Strapi,NextJs\" -c \"blue.bold,green.bold\" \"npm run dev-strapi\" \"npm run dev-next\"",
    "dev-next": "wait-on http://127.0.0.1:1337 && cd next && npm run dev",
    "dev-strapi": "cd strapi && npm run develop"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  }
}
